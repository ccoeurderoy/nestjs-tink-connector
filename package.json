{
  "name": "@algoan/nestjs-tink-connector",
  "version": "0.0.1",
  "description": "Tink connector for Algoan",
  "author": "algoan",
  "license": "UNLICENSED",
  "repository": {
    "url": "https://github.com/algoan/nestjs-tink-connector"
  },
  "scripts": {
    "postinstall": "node-config-ts",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "concurrently --kill-others \"npm run start:fake-server\" \"nest start --watch\"",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:fake-server": "node json-server/server.js",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "publish": "semantic-release",
    "prepare": "husky install"
  },
  "dependencies": {
    "@algoan/nestjs-http-exception-filter": "^1.0.14",
    "@algoan/nestjs-logging-interceptor": "^2.1.12",
    "@algoan/rest": "^2.5.3",
    "@nestjs/axios": "^0.0.8",
    "@nestjs/common": "^8.4.7",
    "@nestjs/core": "^8.4.7",
    "@nestjs/platform-express": "^8.4.7",
    "class-transformer": "^0.5.1",
    "class-transformer-validator": "^0.9.1",
    "class-validator": "^0.13.2",
    "nest-winston": "^1.7.0",
    "node-config-ts": "^3.1.0",
    "qs": "^6.11.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "winston": "^3.8.1"
  },
  "peerDependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@algoan/eslint-config": "^1.1.5",
    "@codedependant/semantic-release-docker": "^4.1.0",
    "@commitlint/config-conventional": "^17.0.3",
    "@nestjs/cli": "^8.2.8",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^8.4.7",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.7",
    "@types/node": "^18.7.9",
    "@types/qs": "^6.9.7",
    "@types/supertest": "^2.0.12",
    "axios": "^0.27.2",
    "commitlint": "^17.0.3",
    "concurrently": "^7.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "delay": "^5.0.0",
    "eslint-plugin-nestjs": "^1.2.3",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "json-server": "^0.17.0",
    "lint-staged": "^13.0.3",
    "nock": "^13.2.9",
    "prettier": "^2.7.1",
    "semantic-release": "^19.0.3",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "tslint": "^6.1.3",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --list-different \"{src,test}/**/*.ts\""
    ]
  }
}
